        <arguments>
          <argument>
            <kind>1</kind>
            <string>//keyboard input
//h_key=keyboard_check(ord("D"))-keyboard_check(ord("A"))
j_key=keyboard_check_pressed(ord("Z"))
d_key=keyboard_check_pressed(ord("X"))


//dashing
if d_key and !dashing{
    dashing=true
    alarm[1]=dashTime
    accSp=0
    vsp=0
    hsp=dashSp
    audio_play_sound(snd_dash,0,false)
}


//speed
vsp+=accSp
if place_meeting(x,y+permAcc,obj_collision){
    if !audio_is_playing(snd_footsteps) and hsp!=0{
        audio_play_sound(snd_footsteps,0,false)
    }
    if j_key{
        vsp=-jumpSp
        audio_play_sound(snd_jump,0,false)
        dashing=false
        accSp=permAcc
        alarm[1]=-1
        hsp=walkSp
    }
}

if vsp&gt;maxFallSp{
    vsp=maxFallSp
}

if dashing{
    hsp=dashSp
}
else if room==rm_game{
    hsp=walkSp
}
if room==rm_menu and !dashing{
    hsp=0
}

if dashing{
    sprite_index=spr_playerDash
}
else if !place_meeting(x,y+permAcc,obj_collision){
    sprite_index=spr_playerAir
}
else if hsp!=0{
    if sprite_index!=spr_playerRun{
        sprite_index=spr_playerRun
    }
}
else{
    sprite_index=spr_playerIdle
}

//movement/collisions
hInc=(permAcc*sign(hsp))
vInc=(permAcc*sign(vsp))
absH=abs(hsp)
absV=abs(vsp)
spMax=max(absH,absV)
absVInc=abs(vInc)
absHInc=abs(hInc)
if absH&gt;absV{
    vInc=(absV/absH)*vInc
}
if absV&gt;absH{
    hInc=(absH/absV)*hInc
}
for(var i=0;i&lt;spMax;i+=permAcc){
    if !place_meeting(x+hInc,y,obj_collision){
        x+=hInc
    }
    else{
        hsp=0
    }
    if !place_meeting(x,y+vInc,obj_collision){
        y+=vInc
    }
    else{
        vsp=0
    }
}

if !place_meeting(x,y+permAcc,obj_collision) or hsp==0{
    if audio_is_playing(snd_footsteps){
        audio_stop_sound(snd_footsteps)
    }
} 

floorX=floor(x)
floorY=floor(y)

if room==rm_game and x&gt;cameraDis{
    view_xview[0]=floorX-cameraDis
}

if place_meeting(floorX,floorY,obj_play) and !started{
    alarm[0]=playTime
    with obj_play{
        instance_destroy()
    }
    started=true
}

if (place_meeting(floorX,floorY,obj_spikeball) or floorY&gt;=room_height) and !hurt and !started{
    alarm[2]=30
    audio_play_sound(snd_hurt,0,false)
    hurt=true
}   

if place_meeting(floorX,floorY,obj_goal){
    alarm[0]=playTime
    with obj_goal{
        instance_destroy()
    }
    started=true
} 

</string>
          </argument>
